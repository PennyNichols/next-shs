{
  "name": "shs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev",
    "dev:staging": "dotenv -e .env.staging -- next dev",
    "dev:prod": "dotenv -e .env.prod -- next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:debug": "eslint . --debug",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "deploy:dev": "firebase use dev && cp firestore_rules/dev.rules firestore.rules && cp storage_rules/dev.rules storage.rules && firebase deploy",
    "deploy:staging": "firebase use staging && cp firestore_rules/staging.rules firestore.rules && cp storage_rules/staging.rules storage.rules && firebase deploy",
    "deploy:prod": "firebase use prod && cp firestore_rules/prod.rules firestore.rules && cp storage_rules/prod.rules storage.rules && firebase deploy"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "5.17.1",
    "@mui/material": "5.17.1",
    "@next/bundle-analyzer": "^15.3.5",
    "axios": "^1.7.9",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.3.2",
    "firebase-tools": "^14.9.0",
    "lodash": "^4.17.21",
    "next": "^15.2.3",
    "react": "^19",
    "react-dom": "^19",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-hook-form": "^7.58.1",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^4.0.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.3",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "prettier": "^3.5.3"
  }
}